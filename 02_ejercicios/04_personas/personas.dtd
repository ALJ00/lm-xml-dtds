<!ELEMENT personas (persona +)>

        <!ELEMENT persona (nombre,email+,parentesco?)>
        <!-- + [1..n] , * [0...n] -->

        <!ELEMENT nombre (#PCDATA)>

        <!ATTLIST nombre dni CDATA #REQUIRED
                edad CDATA #IMPLIED
                sexo  ( M | H ) #REQUIRED>
        <!ELEMENT email (#PCDATA)>
        <!ELEMENT parentesco (#PCDATA)>
        <!ATTLIST parentesco
                informacion  CDATA( padre de | hermano de )CDATA #IMPLIED>